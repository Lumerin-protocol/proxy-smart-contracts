{
  "name": "marketplace",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -rf data generated build",
    "codegen": "graph codegen",
    "prepare-local": "mustache config/localhost.json subgraph.template.yaml > subgraph.yaml",
    "prepare-dev": "mustache config/arbitrum-sepolia.json subgraph.template.yaml > subgraph.yaml",
    "prepare-stg": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml",
    "prepare-mainnet": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ hr-futures",
    "create-local": "graph create --node http://localhost:8020/ marketplace",
    "remove-local": "graph remove --node http://localhost:8020/ marketplace",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 --version-label 0 marketplace",
    "setup-local": "yarn prepare-local && yarn codegen && yarn build && yarn create-local && yarn deploy-local",
    "test": "graph test",
    "indexer": "docker-compose up"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "0.37.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "viem": "^2.37.9"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.98.1",
    "matchstick-as": "0.6.0",
    "mustache": "^4.2.0",
    "typescript": "^5.9.3"
  }
}